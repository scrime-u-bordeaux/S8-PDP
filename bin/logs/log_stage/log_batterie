%Coefficients de corrélation entre batterie et autres pistes à temps aléatoire%

for m=randi([1,44])
  M=(m-1)*0.743
  imagesc(C(:,:,m)),colormap(viridis),caxis([0,1]),colorbar
  set(gca,'XTickLabel',['Intro';'Basse';'Clavier gauche';'Clavier droite';'Cuivres';'Voix'])
