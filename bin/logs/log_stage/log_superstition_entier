%Coefficient entre 2 pistes aléatoires sans précision%

for l=randi([1,21])
  for m=randi([1,99])
  M=(m-1)*0.743
    imagesc(AA(:,l,m)),colormap(viridis),caxis([0,1]),colorbar
    xlabel('Corrélation entre 2 pistes')
    set(gca,'ytick',[])
    title(sprintf("Coefficients de corrélation pour l= %1.0f de Superstition \n à m= %1.0f secondes",l,M))
  end
end
m,l,M


%Coefficient entre 2 pistes aléatoires avec précision%
 
for l=randi([1,21])
  for m=randi([1,99])
  M=(m-1)*0.743
    imagesc(AA(:,l,m)),colormap(viridis),caxis([0,1]),colorbar
    set(gca,'ytick',[])
      if (l==1)
         title(sprintf("Coefficient de corrélation pour intro et batterie de Superstition \n à %1.0f secondes",M))
      elseif (l==2) 
        title(sprintf("Coefficient de corrélation pour intro et basse de Superstition \n à %1.0f secondes",M))
      elseif (l==3)
        title(sprintf("Coefficient de corrélation pour batterie et basse de Superstition \n à %1.0f secondes",M))
      elseif (l==4)
        title(sprintf("Coefficient de corrélation pour intro et clavier gauche de Superstition \n à %1.0f secondes",M))
      elseif (l==5)
        title(sprintf("Coefficient de corrélation pour batterie et clavier gauche de Superstition \n à %1.0f secondes",M))
      elseif (l==6)
        title(sprintf("Coefficient de corrélation pour basse et clavier gauche de Superstition \n à %1.0f secondes",M))
      elseif (l==7)
        title(sprintf("Coefficient de corrélation pour intro et clavier droite de Superstition \n à %1.0f secondes",M))
      elseif (l==8)
        title(sprintf("Coefficient de corrélation pour batterie et clavier droite de Superstition \n à %1.0f secondes",M))
      elseif (l==9)
        title(sprintf("Coefficient de corrélation pour basse et clavier droite de Superstition \n à %1.0f secondes",M))
      elseif (l==10)
        title(sprintf("Coefficient de corrélation pour clavier gauche et clavier droite de Superstition \n à %1.0f secondes",M))
      elseif (l==11)
        title(sprintf("Coefficient de corrélation pour intro et cuivres de Superstition \n à %1.0f secondes",M))
      elseif (l==12)
        title(sprintf("Coefficient de corrélation pour batterie et cuivres de Superstition \n à %1.0f secondes",M))
      elseif (l==13)
        title(sprintf("Coefficient de corrélation pour basse et cuivres de Superstition \n à %1.0f secondes",M))
      elseif (l==14)
        title(sprintf("Coefficient de corrélation pour clavier gauche et cuivres de Superstition \n à %1.0f secondes",M))
      elseif (l==15)
        title(sprintf("Coefficient de corrélation pour clavier droite et cuivres de Superstition \n à %1.0f secondes",M))
      elseif (l==16)
        title(sprintf("Coefficient de corrélation pour intro et voix de Superstition \n à %1.0f secondes",M))
      elseif (l==17)
        title(sprintf("Coefficient de corrélation pour batterie et voix de Superstition \n à %1.0f secondes",M))
      elseif (l==18)
        title(sprintf("Coefficient de corrélation pour basse et voix de Superstition \n à %1.0f secondes",M))
      elseif (l==19)
        title(sprintf("Coefficient de corrélation pour clavier gauche et voix de Superstition \n à %1.0f secondes",M))
      elseif (l==20)
        title(sprintf("Coefficient de corrélation pour clavier droite et voix de Superstition \n à %1.0f secondes",M))
      elseif (l==21)
        title(sprintf("Coefficient de corrélation pour cuivres et voix de Superstition \n à %1.0f secondes",M))
      end
  end
end
