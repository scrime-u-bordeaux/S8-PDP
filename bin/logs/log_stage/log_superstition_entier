AA=permute(reshape(A,99,21,1),[3,2,1])
AA(:,:,88)
imagesc(AA(:,:,56)),colorbar


%Coefficients pour chaque piste à un temps aléatoire%

for m=randi([1,99]) %nb d'images
  M=(m-1)*0.743 %nb de secondes
  imagesc(AA(:,:,m)),colormap(viridis),caxis([0,1]),colorbar
  set(gca,'ytick',[])
  set(gca,'Xtick', [1:21] )
  set(gca,'XTickLabel',["Intro/\nBatterie";"Intro/\nBasse";"Batterie/\nBasse";"Intro/\nCG";"Batterie/\nCG";"Basse/\nCG";"Intro/\nCD";"Batterie/\nCD";"Basse/\nCD";"CG/\nCD"; "Intro/\nCuivres";"Batterie/\nCuivres";"Basse/\nCuivres";"CG/\nCuivres";"CD/\nCuivres";"Intro/\nVoix";"Batterie/\nVoix";"Basse/\nVoix";"CG/\nVoix";"CD/\nVoix";"Cuivres/\nVoix"])
  xlim([1,21])
  title(sprintf("Coefficients de corrélation entre chaque piste de Superstition à  %1.0f secondes",M))
end
m,M



%Test légende%

imagesc(AA(:,:,99)),caxis([0,1]),colorbar
L = line(ones(21),ones(21), 'LineWidth',2);               % generate line 
set(L,{'color'},mat2cell(viridis(21),ones(1,21),3));            % set the colors according to cmap
